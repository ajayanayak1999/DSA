 public static int[] Rearrange (int arr[], int n) {
        // Complete the function
        for(int i=0;i<n;)
        {
            if(arr[i]>=0 &&arr[i]!=i)
            {
                int temp=arr[arr[i]];
                arr[arr[i]]=arr[i];
                arr[i]=temp;
            }
            else
                i++;
        }
        return arr;
    }
    
